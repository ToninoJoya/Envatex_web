# .env — variables de entorno para el backend
# Cambia los valores según tu entorno (no subir a git)

# Ruta a la base de datos (por defecto sqlite local)
DATABASE_URL=sqlite:///app.db

# Entorno de Flask: development / production
FLASK_ENV=development

# Puerto en el que correrá la app
PORT=5000

# Clave secreta de ejemplo (cámbiala en producción)
SECRET_KEY=dev-secret
